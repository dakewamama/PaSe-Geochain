{
  "name": "@civic/solana-gateway-chain-client",
  "version": "0.0.5",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": "git@github.com:civicteam/ociv-gatekeeper.git",
  "description": "Civic Solana Chain Client",
  "dependencies": {
    "@civic/gateway-client-core": "1.1.1",
    "@civic/prove-solana-wallet": "0.4.3",
    "@civic/solana-gateway-ts": "^0.13.0-alpha.0",
    "@solana/web3.js": "^1.95.3",
    "prop-types": "^15.8.1",
    "ramda": "^0.30.0"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint -c ../.eslintrc.json './{src,test}/**/*.ts' --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "test:unit": "TS_NODE_PROJECT=./tsconfig.test.json nyc --reporter=text mocha --timeout 20000 test/unit/**/*.test.ts --exit",
    "test:integration": "TS_NODE_PROJECT=./tsconfig.test.json mocha --require mocha-steps --timeout 30000 test/integration/**/*.test.ts --exit",
    "test": "yarn test:unit && yarn test:integration",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --noEmit false --outDir dist/esm",
    "build:cjs": "tsc --noEmit false --module commonjs --outDir dist/cjs",
    "prepare": "yarn build",
    "script": "ts-node -O '{\"module\":\"commonjs\"}' --transpile-only --files"
  },
  "devDependencies": {
    "@civic/solana-gatekeeper-lib": "^6.1.1",
    "@types/mocha-steps": "^1.3.3",
    "@types/ramda": "^0.30.0",
    "@types/sinon-chai": "^3.2.12",
    "bs58": "^5.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.28.0",
    "mocha-steps": "^1.3.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.10.2",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.6.4"
  },
  "gitHead": "f6282f6628701fc1bfdf53aae2202ccb3f463570"
}
